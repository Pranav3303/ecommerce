{
    "version": 3,
    "builds": [
      {
        "src": "ecommerce_proj/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.13.1"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "ecommerce_proj/wsgi.py"
      }
    ]
}